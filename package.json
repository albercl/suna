{
    "name": "suna",
    "version": "0.0.1",
    "description": "A discord bot written in TypeScript",
    "main": "Index.ts",
    "scripts": {
        "build": "tsc",
        "start": "node -r source-map-support/register dist/Index.js",
        "test": "jest --run-in-band --coverage test/**/*.ts",
        "debug": "nodemon --legacy-watch --watch src --exec node --inspect=0.0.0.0:9229 --inspect-brk -r ts-node/register src/Index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/albercl/suna.git"
    },
    "keywords": [
        "discord"
    ],
    "author": "albercl",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/albercl/suna/issues"
    },
    "homepage": "https://github.com/albercl/suna#readme",
    "dependencies": {
        "discord.js": "^14.5.0",
        "source-map-support": "^0.5.21",
        "typescript": "^4.8.4",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/jest": "^29.2.0",
        "@types/node": "^18.8.0",
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "eslint": "^8.24.0",
        "jest": "^29.2.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1"
    }
}
